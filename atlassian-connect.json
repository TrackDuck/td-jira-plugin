{
    "key": "com.trackduck.jira-plugin",
    "name": "TrackDuck",
    "version": "1.0",
    "description": "JIRA add-on for integration with TrackDuck",
    "vendor": {
        "name": "TrackDuck",
        "url": "https://trackduck.com"
    },
    "baseUrl": "https://tdcdn.blob.core.windows.net/jira/jira-plugin-1.0.jar",
    "authentication": {
        "type": "none"
    },
    "modules": {
        "webItems": [
            {
                "url": "https://app.trackduck.com/auth/boarding/jira",
                "location": "system.top.navigation.bar",
                "context": "addon",
                "weight": 100,
                "key": "trackDuckButton",
                "condition": {
                    "condition": "user_is_logged_in"
                },
                "name": {
                    "value": "TrackDuck"
                },
                "tooltip": {
                    "value": "Setup TrackDuck integration"
                }
            }
        ]
    }
}